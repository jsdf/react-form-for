{
  "name": "react-form-for",
  "version": "1.0.1",
  "description": "A simple form builder for React in the style of Rails' form_for",
  "main": "./lib/index.js",
  "scripts": {
    "test": "npm run prepublish && flow check && jest",
    "jest": "jest",
    "watch": "node tasks/watch test src/ example/",
    "demo": "make demo",
    "demo-run": "open http://0.0.0.0:8080/ && http-server example/",
    "umd": "browserify ./lib/index.js --external react --standalone ReactFormFor > dist/react-form-for.js",
    "dist": "npm run umd && git add dist/ && git commit -m 'umd distribution rebuild' && git push origin master",
    "prepublish": "jsx --strip-types --harmony src/ lib/"
  },
  "author": "James Friend <james@jsdf.co>",
  "license": "MIT",
  "homepage": "https://github.com/jsdf/react-form-for",
  "bugs": "https://github.com/jsdf/react-form-for/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsdf/react-form-for.git"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules"
    ]
  },
  "devDependencies": {
    "react": "0.12.0",
    "react-tools": "^0.12.0",
    "object.assign": "^1.0.1",
    "jest-cli": "^0.1.18",
    "jquery": "^2.1.1",
    "reactify": "^0.17.0",
    "js-beautify": "^1.5.4",
    "chokidar": "^0.11.1"
  },
  "dependencies": {
    "xtend": "^4.0.0",
    "lodash.memoize": "^2.4.1",
    "react-tools": "^0.12.1"
  }
}
